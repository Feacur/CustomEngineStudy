! # entity
	p assets/prefabs/suzanne.entity
		o Visual
			l 1
		~
Lua_Script
	u script_rotate
> # child
	p assets/prefabs/suzanne.entity
		o Transform
			p 1 1.5 1
			s 0.25 0.25 0.25
		Visual
			l 1
		~
	p assets/prefabs/suzanne.entity
		o Transform
			p -1 1.5 1
			s 0.25 0.25 0.25
		Lua_Script
			u script_rotate
		> # child
			p assets/prefabs/suzanne.entity
				o Transform
					p 2 1.5 1
				~
